<aside class="block truncate col-span-1 mb-12">
  <%= render Layout::Subheading.new(title: @title) %>

  <ul class="list code">
    <% @modules.each do |m| %>
      <li><%= active_link_to m.qualified_name, gem_module_path(@gem.name, @gem.version, m.qualified_name) %></li>
    <% end %>
  </ul>
</aside>
